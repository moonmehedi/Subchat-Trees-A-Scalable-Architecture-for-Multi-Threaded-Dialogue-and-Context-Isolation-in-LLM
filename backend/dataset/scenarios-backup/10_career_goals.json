{
  "scenario_name": "Career Goals & Aspirations",
  "description": "Tests tracking of professional goals and timeline",
  "tree_type": "main_with_subchats",
  "conversations": [
    {
      "step": 1,
      "action": "send_message",
      "node_type": "main",
      "message": "My ultimate career goal is to become a Principal Engineer at a FAANG company",
      "expected_response_contains": ["Principal Engineer", "FAANG", "career goal"]
    },
    {
      "step": 2,
      "action": "send_message",
      "node_type": "main",
      "message": "I want to achieve this within the next 5 years by 2030",
      "expected_response_contains": ["5 years", "2030"]
    },
    {
      "step": 3,
      "action": "send_message",
      "node_type": "main",
      "message": "I currently work as a Senior Software Engineer at a mid-size startup",
      "expected_response_contains": ["Senior Software Engineer", "startup"]
    },
    {
      "step": 4,
      "action": "send_message",
      "node_type": "main",
      "message": "My current salary is $145,000 per year",
      "expected_response_contains": ["$145,000", "salary"]
    },
    {
      "step": 5,
      "action": "create_subchat",
      "parent_node": "main",
      "subchat_name": "Skills to Learn",
      "message": "I need to master system design and distributed systems",
      "expected_response_contains": ["system design", "distributed systems", "master"]
    },
    {
      "step": 6,
      "action": "send_message",
      "node_type": "subchat_1",
      "message": "I'm currently taking a course on Kubernetes and Docker",
      "expected_response_contains": ["Kubernetes", "Docker", "course"]
    },
    {
      "step": 7,
      "action": "send_message",
      "node_type": "subchat_1",
      "message": "I want to get AWS Solutions Architect certification by end of this year",
      "expected_response_contains": ["AWS", "Solutions Architect", "certification"]
    },
    {
      "step": 8,
      "action": "send_message",
      "node_type": "subchat_1",
      "message": "I also need to improve my leadership and mentoring skills",
      "expected_response_contains": ["leadership", "mentoring", "skills"]
    },
    {
      "step": 9,
      "action": "create_subchat",
      "parent_node": "main",
      "subchat_name": "Side Projects",
      "message": "I'm building an open-source project to demonstrate my skills",
      "expected_response_contains": ["open-source", "project", "skills"]
    },
    {
      "step": 10,
      "action": "send_message",
      "node_type": "subchat_2",
      "message": "It's a real-time collaborative code editor like Google Docs but for code",
      "expected_response_contains": ["real-time", "collaborative", "code editor"]
    },
    {
      "step": 11,
      "action": "send_message",
      "node_type": "subchat_2",
      "message": "I'm using WebSockets, CRDT algorithms, and React for the frontend",
      "expected_response_contains": ["WebSockets", "CRDT", "React"]
    },
    {
      "step": 12,
      "action": "send_message",
      "node_type": "subchat_2",
      "message": "I hope to get 10,000 stars on GitHub with this project",
      "expected_response_contains": ["10,000 stars", "GitHub"]
    },
    {
      "step": 13,
      "action": "switch_to_node",
      "node_type": "main",
      "message": "What's my ultimate career goal?",
      "expected_response_contains": ["Principal Engineer", "FAANG"],
      "should_retrieve": true,
      "expected_retrieval": ["Principal Engineer", "FAANG"],
      "should_not_contain": ["Kubernetes", "open-source", "collaborative"]
    },
    {
      "step": 14,
      "action": "send_message",
      "node_type": "main",
      "message": "What's my current salary?",
      "expected_response_contains": ["$145,000"],
      "should_retrieve": true,
      "expected_retrieval": ["$145,000"]
    },
    {
      "step": 15,
      "action": "switch_to_node",
      "node_type": "subchat_2",
      "message": "What's my side project about?",
      "expected_response_contains": ["collaborative", "code editor"],
      "should_retrieve": true,
      "expected_retrieval": ["collaborative code editor"],
      "should_not_contain": ["Principal Engineer", "$145,000"]
    }
  ]
}
